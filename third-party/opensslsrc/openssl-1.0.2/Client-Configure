#!/bin/sh

##  Onavo uses rc2, so please don't add 'no-rc2'
DEPS=" no-asm \
 no-bf \
 no-camellia \
 no-cast \
 no-comp \
 no-dtls1 \
 no-engine \
 no-gmp \
 no-gost \
 no-heartbeats \
 no-hw \
 no-idea \
 no-jpake \
 no-krb5 \
 no-libunbound \
 no-md2 \
 no-md4 \
 no-mdc2 \
 no-psk \
 no-rc5 \
 no-rfc3779 \
 no-ripemd \
 no-sctp \
 no-seed \
 no-sha0 \
 no-srp \
 no-ssl2 \
 no-ssl3 \
 no-ssl3-method \
 no-store \
 no-txt_db \
 no-unit-test \
 no-whirlpool \
"

echo "[Config] ./Configure $* $DEPS"
./Configure $* $DEPS
